{
    "extends": [ "google", "plugin:react/recommended", "prettier" ],
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "plugins": [ "react-hooks", "prettier" ],
    "rules": {
        "prettier/prettier": "warn",
        "require-jsdoc": 0,
        "react/display-name": 0,
        "guard-for-in":"off"
    },
    "settings": {
        "react": {
          "version": "detetect"
        }
    }
}